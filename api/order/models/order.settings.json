{
  "kind": "collectionType",
  "collectionName": "orders",
  "info": {
    "name": "order",
    "description": ""
  },
  "options": {
    "increments": true,
    "timestamps": true,
    "draftAndPublish": true
  },
  "attributes": {
    "status": {
      "type": "enumeration",
      "enum": [
        "ESPERA",
        "PROCESO",
        "TRANSITO",
        "ENTREGADO",
        "CONCRETADO",
        "CANCELADO"
      ],
      "required": true
    },
    "approved_at": {
      "type": "date",
      "required": true
    },
    "user_id": {
      "plugin": "users-permissions",
      "model": "user",
      "via": "orders"
    },
    "order_stocks": {
      "collection": "order-stock",
      "via": "order"
    },
    "order_coupons": {
      "collection": "order-coupon",
      "via": "order_id"
    },
    "reference": {
      "type": "string"
    },
    "method": {
      "type": "string"
    },
    "address_id": {
      "model": "addresses",
      "via": "orders"
    }
  }
}
